<Window x:Class="GEditor.Views.InputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GEditor.Views"
        mc:Ignorable="d"
        Title="创建文件" SizeToContent="WidthAndHeight" MinWidth="200">
    <StackPanel Margin="10">
        <TextBox x:Name="FileNameTextBox" Margin="0,0,0,10"/>
        <Button Content="确认" Click="ConfirmButton_Click"/>
    </StackPanel>
</Window>
